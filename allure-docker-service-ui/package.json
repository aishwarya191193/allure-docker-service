{
  "name": "allure-docker-service-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "axios": "0.19.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "universal-cookie": "4.0.3"
  },
  "devDependencies": {},
  "scripts": {
    "start": "REACT_APP_ALLURE_DOCKER_URL=http://localhost:5050 PORT=4000 react-scripts start",
    "start:security": "REACT_APP_ALLURE_DOCKER_URL=http://localhost:5050 PORT=4000 REACT_APP_ENABLE_SECURITY=1 react-scripts start",
    "build": "REACT_APP_ALLURE_DOCKER_URL=../ PUBLIC_URL=/ui react-scripts build",
    "build:security": "REACT_APP_ALLURE_DOCKER_URL=../ REACT_APP_ENABLE_SECURITY=1 PUBLIC_URL=/ui react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
